{% extends 'base.html.twig' %}

{% block content %}
    <h1>Create new Category</h1>

    <div class="row">
        <div class="col-md-6">
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
            {{ form_end(form) }}
        </div>
    </div>

    <a href="{{ path('category_index') }}" class="btn btn-secondary mt-3">Back to list</a>
{% endblock %}
